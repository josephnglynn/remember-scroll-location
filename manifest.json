{
  "manifest_version": 2,
  "name": "remember-scroll-location",
  "version": "0.1",
  "description": "remember-scroll-location description",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": ["tabs",  "<all_urls>"],
  "icons": {
    "64": "icon.png"
  },
  "browser_action": {
    "default_title": "remember-scroll-location (browserAction)",
    "default_icon": {
      "64": "icon.png"
    }
  }
}